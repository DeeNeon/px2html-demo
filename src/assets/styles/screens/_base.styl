@import 'https://fonts.googleapis.com/css?family=Roboto:300,400,500,700'

body
  font-family 'Roboto', Calibri, Arial, sans-serif
  color #43484D

h3
 font-weight 400

a
a:active
a:focus
  color #f0f0f0
  text-decoration none
